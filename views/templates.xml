<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="assets_backend" name="cms owl assets back" inherit_id="web.assets_backend">
            <xpath expr="//script[last()]" position="after">

                <!--components-->
                <link rel="stylesheet" type="text/scss" href="/cms_frontend_owl/static/src/components/app/app.scss"/>
                <link rel="stylesheet" type="text/scss" href="/cms_frontend_owl/static/src/components/task/task.scss"/>

                <script type="text/javascript" src="/cms_frontend_owl/static/src/components/task/task.js"></script>
                <script type="text/javascript" src="/cms_frontend_owl/static/src/components/app/app.js"></script>

                <script type="text/javascript" src="/cms_frontend_owl/static/src/widget/todo.js"></script>
            </xpath>
        </template>
        <template id="assets_frontend" name="cms owl assets front" inherit_id="web.assets_frontend">
            <xpath expr="//script[last()]" position="after">

                <!--components-->
                <link rel="stylesheet" type="text/scss" href="/cms_frontend_owl/static/src/components/app/app.scss"/>
                <link rel="stylesheet" type="text/scss" href="/cms_frontend_owl/static/src/components/task/task.scss"/>

                <script type="text/javascript" src="/cms_frontend_owl/static/src/components/task/task.js"></script>
                <script type="text/javascript" src="/cms_frontend_owl/static/src/components/app/app.js"></script>

                <script type="text/javascript" src="/cms_frontend_owl/static/src/widget/todo.js"></script>
            </xpath>
        </template>
        <template id="cms_frontend_owl.cms_layout" name="CMS Layout">
            <t t-call="web.frontend_layout">
                <t t-set="html_data" t-value="{'style': 'height: 100%;'}"/>
                <t t-set="body_classname" t-value="'bg-100'"/>
                <t t-set="no_header" t-value="True"/>
                <t t-set="no_footer" t-value="True"/>
                <t t-set="x_icon" t-value="x_icon"/>
                <div class="container py-5">
                    <App></App>
                </div>
            </t>
        </template>
    </data>
</odoo>
